<schema adapter="testpagri">

	<element tablename="test_pagricommodity_type">
		<field name="objid" primary="true" prefix="CTYP" />
		<field name="parentid" visible="false" required="true"  />
		<field name="parentname" visible="false" required="true" />
		<field name="name" required="true" searchable="true" />
		<field name="code" required="false" searchable="true" />
		<field name="description" required="false" searchable="true" />

		<complex name="parent" ref="test_pagricommodity" jointype="many-to-one" includefields="code,name,objid" 
			required="false">
			<key field="parentid" target="objid" />
		</complex>		
	</element>

</schema>