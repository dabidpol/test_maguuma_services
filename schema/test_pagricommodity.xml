<schema adapter="testpagri">

	<element tablename="test_pagricommodity">
		<field name="objid" primary="true" prefix="COM" />
		<field name="name" required="true" searchable="true" indexed="true"/>
		<field name="code" required="false" searchable="true" indexed="true"/>
		<field name="description" required="false" searchable="true" indexed="true"/>

		<complex name="items" ref="test_pagricommodity_type" jointype="one-to-many" orderBy="name">
			<key field="objid" target="parentid" /> 		
		</complex> 
	</element>

</schema>